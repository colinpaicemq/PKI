//IBMRACF  JOB 1,MSGCLASS=H RESTART=DEL
//DEF EXEC PGM=IKJEFT01,REGION=0M
//SYSPRINT DD SYSOUT=*
//SYSTSPRT DD SYSOUT=*
//SYSTSIN DD *
DELUSER  WEB2
ADDGROUP COLWEBG  OMVS( AUTOGID )
ADDUSER  WEB2     NAME('COLWEBG')    NOPASSWORD -
   DFLTGRP(COLWEB)             -
   OMVS(AUTOUID                             -
        Program('/bin/sh') home('/u/mqweb3/') -
       )

CONNECT WEB2    GROUP(COLWEB )
RALTER  STARTED COLWEB.* STDATA(USER(WEB2))

SETROPTS RACLIST(STARTED ) REFRESH

PERMIT IRR.RPKISERV.PKIADMIN  CLASS(FACILITY)-
   ID(WEB2   ) ACCESS(read   )
setropts raclist(facility) refresh
